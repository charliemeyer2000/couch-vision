    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "create-foxglove-extension": "1.0.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.7.2"
